{
  "name": "@klubben/maser-js",
  "private": false,
  "version": "0.0.8",
  "license": "0BSD",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "lint-staged": "lint-staged",
    "lint": "eslint -c eslint.config.mjs",
    "test": "vitest --run ./",
    "test:watch": "vitest ./",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc -w --noEmit",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006 --ci",
    "build-storybook": "storybook build"
  },
  "repository": "https://github.com/klubben/maser-js",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "dependencies": {
    "@tweenjs/tween.js": "^25.0.0",
    "lodash": "^4.17.21",
    "pixi.js": "^8.9.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3",
    "@eslint/js": "^9.17.0",
    "@storybook/addon-essentials": "^8.6.8",
    "@storybook/addon-interactions": "^8.6.8",
    "@storybook/blocks": "^8.6.8",
    "@storybook/html": "^8.6.8",
    "@storybook/html-vite": "^8.6.8",
    "@storybook/test": "^8.6.8",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.5",
    "eslint": "^9.17.0",
    "eslint-plugin-storybook": "^0.11.6",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.2.11",
    "prettier": "3.4.2",
    "storybook": "^8.6.8",
    "tsx": "^4.19.3",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "vite": "^6.0.9",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-glsl": "^1.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.8",
    "vitest-canvas-mock": "^0.3.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint -c eslint.config.mjs --max-warnings 0",
      "prettier --write"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
